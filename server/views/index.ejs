<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel="stylesheet" media="screen" href="stylesheets/bootstrap.css">
    <link rel="stylesheet" media="screen" href="stylesheets/typeahead.js-bootstrap.css">
    <script src="javascripts/jquery-1.10.1.min.js" type="text/javascript"></script>
    <script src="javascripts/typeahead.min.js" type="text/javascript"></script>
  </head>
  <body>

     <script type="text/javascript"
          src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDh-VeILk5cC41U0FsOU02psAJHBCk1BwI&sensor=false">
        </script>

    	<script type="text/javascript">

          	function initialize() {
           		var latlng = new google.maps.LatLng(53.197, -2.368);
             	var mapOptions = {
                  center: latlng,
                  zoom: 11,
                  mapTypeId: google.maps.MapTypeId.ROADMAP
                };
                var map = new google.maps.Map(document.getElementById("map-canvas"),
                    mapOptions);
                var myMarker = new google.maps.Marker({
                	  position: latlng,
                	  map: map,
                	  title:"HCCS"
                	});
           	}
          	google.maps.event.addDomListener(window, 'load', initialize);
        </script>

        <div class="span12">
    	    <div class="row">
    		    <div class="span7">
    		        <div id="map-canvas" style="width:526px; height:400px;"></div>
 			    </div>
			    <div class="span5" align="center">
				    <h3 style="padding: 50px ">Select your school</h3>

                </div>
    	    </div>
	    </div>

  </body>
</html>